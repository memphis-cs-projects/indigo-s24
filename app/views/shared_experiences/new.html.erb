<!-- app/views/shared_experiences/new.html.erb -->
<h1>New Shared Experience</h1>

<%= form_with(model: @shared_experience, url: shared_experiences_path, method: :post) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :description %>
  <%= form.text_area :description %>

  <%= form.submit 'Create Shared Experience' %>
<% end %>
